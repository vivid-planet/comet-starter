{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "workspaces": {
        ".": {
            "entry": ["./src/loader.ts"],
            "ignore": ["./codegen.ts", "./schema.gql", "./src/**/generated/**/*.{ts,tsx}", "./src/**/*.cometGen.ts"],
            "ignoreDependencies": ["@swc/plugin-emotion"],
            "project": ["./src/**/*.{ts,tsx}"]
        },
        "server": {
            "ignoreDependencies": ["http-proxy-middleware"],
            "project": ["**/*.js"]
        }
    }
}
