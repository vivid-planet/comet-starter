{
    "name": "starter-admin",
    "private": true,
    "scripts": {
        "admin-generator": "rimraf 'src/*/generated' && comet-admin-generator generate crud-generator-config.ts && comet-admin-generator future-generate",
        "build": "npm run intl:compile && run-p gql:types generate-block-types && NODE_ENV=production vite build",
        "generate-block-types": "comet generate-block-types --inputs",
        "generate-block-types:watch": "chokidar -s \"block-meta.json\" -c \"npm run generate-block-types\"",
        "gql:types": "graphql-codegen",
        "gql:watch": "graphql-codegen --watch",
        "postinstall": "cd server && npm install",
        "intl:compile": "run-p intl:compile:starter-admin intl:compile:comet",
        "intl:compile:comet": "formatjs compile-folder --format simple --ast lang/comet-lang lang-compiled/comet-lang",
        "intl:compile:starter-admin": "formatjs compile-folder --format simple --ast lang/starter-lang/admin lang-compiled/starter-admin",
        "intl:extract": "formatjs extract \"src/**/*.ts*\" --ignore ./**.d.ts --out-file lang-extracted/en.json --format simple --throws",
        "prelint": "npm run intl:compile && run-p gql:types generate-block-types",
        "lint": "run-p lint:eslint lint:tsc intl:extract lint:knip",
        "lint:ci": "npm run lint && npm run lint:generated-files-not-modified",
        "lint:eslint": "eslint --max-warnings 0 --ext .ts,.tsx,.js,.jsx,.json,.md src/ package.json",
        "lint:generated-files-not-modified": "npm run admin-generator && git diff --exit-code HEAD -- src/**/generated",
        "lint:knip": "knip --exports --files --dependencies --tags=-knipignore",
        "lint:prettier": "npx prettier --check './**/*.{js,json,md,yml,yaml}'",
        "lint:tsc": "tsc --project .",
        "preview": "npm run build && vite preview",
        "serve": "node server",
        "start": "npm run intl:compile && run-p gql:types generate-block-types && dotenv -e .env.secrets -e .env.local -e .env -e .env.site-configs -- vite"
    },
    "dependencies": {
        "@apollo/client": "^3.11.9",
        "@comet/admin": "7.13.0",
        "@comet/admin-icons": "7.13.0",
        "@comet/admin-theme": "7.13.0",
        "@comet/blocks-admin": "7.13.0",
        "@comet/cms-admin": "7.13.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@fontsource-variable/roboto-flex": "^5.1.1",
        "@mui/material": "^5.16.14",
        "@mui/system": "^5.16.14",
        "@mui/x-data-grid": "^5.17.26",
        "@mui/x-data-grid-pro": "^5.17.26",
        "change-case": "^5.4.4",
        "final-form": "^4.20.10",
        "graphql": "^15.10.1",
        "history": "^4.10.1",
        "lodash.isequal": "^4.5.0",
        "rdndmb-html5-to-touch": "^8.1.2",
        "react": "^17.0.2",
        "react-app-polyfill": "^2.0.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dnd-multi-backend": "^8.1.2",
        "react-dom": "^17.0.2",
        "react-final-form": "^6.5.9",
        "react-intl": "^6.8.9",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4"
    },
    "devDependencies": {
        "@comet/cli": "7.13.0",
        "@comet/eslint-config": "7.13.0",
        "@cspell/eslint-plugin": "^8.17.3",
        "@formatjs/cli": "^6.6.1",
        "@graphql-codegen/add": "^5.0.3",
        "@graphql-codegen/cli": "^5.0.5",
        "@graphql-codegen/fragment-matcher": "^5.1.0",
        "@graphql-codegen/introspection": "^4.0.3",
        "@graphql-codegen/named-operations-object": "^3.1.0",
        "@graphql-codegen/near-operation-file-preset": "^3.0.0",
        "@graphql-codegen/typescript": "^4.1.3",
        "@graphql-codegen/typescript-operations": "^4.4.1",
        "@parcel/watcher": "^2.5.1",
        "@swc/plugin-emotion": "^3.0.13",
        "@types/lodash.isequal": "^4.5.8",
        "@types/node": "^22.13.2",
        "@types/react": "^17.0.83",
        "@types/react-dom": "^17.0.26",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react-swc": "^3.7.2",
        "chokidar-cli": "^3.0.0",
        "dotenv-cli": "^7.4.4",
        "eslint": "^8.57.1",
        "knip": "^5.44.5",
        "npm-run-all2": "^7.0.2",
        "prettier": "^2.8.8",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "typescript": "5.1.6",
        "vite": "^5.4.14",
        "vite-plugin-html": "^3.2.2"
    },
    "engines": {
        "node": "22"
    }
}
