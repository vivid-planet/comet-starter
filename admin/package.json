{
    "name": "starter-admin",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "admin-generator": "rimraf --glob 'src/**/generated' && comet-admin-generator generate",
        "build": "run-s admin-generator intl:compile && run-p gql:types generate-block-types && NODE_ENV=production vite build",
        "check-node-version": "check-node-version --node $(cat ../.nvmrc)",
        "generate-block-types": "comet generate-block-types --inputs",
        "generate-block-types:watch": "chokidar -s \"block-meta.json\" -c \"npm run generate-block-types\"",
        "gql:types": "graphql-codegen",
        "gql:watch": "graphql-codegen --watch",
        "intl:compile": "run-p intl:compile:starter-admin intl:compile:comet",
        "intl:compile:comet": "formatjs compile-folder --format simple --ast lang/comet-lang lang-compiled/comet-lang",
        "intl:compile:starter-admin": "formatjs compile-folder --format simple --ast lang/starter-lang/admin lang-compiled/starter-admin",
        "intl:extract": "formatjs extract \"src/**/*.ts*\" --ignore ./**.d.ts --out-file lang-extracted/en.json --format simple --throws",
        "prelint": "npm run intl:compile && run-p gql:types generate-block-types",
        "lint": "npm run admin-generator && run-p lint:eslint lint:tsc intl:extract lint:knip",
        "lint:ci": "npm run lint",
        "lint:eslint": "eslint --max-warnings 0 --ext .ts,.tsx,.js,.jsx,.json,.md src/ package.json",
        "lint:knip": "knip --exports --files --dependencies --tags=-knipignore",
        "lint:prettier": "npx prettier --check './**/*.{js,json,md,yml,yaml}'",
        "lint:tsc": "tsc --project .",
        "postinstall": "cd server && npm install",
        "preview": "npm run build && vite preview",
        "serve": "node server",
        "start": "npm run check-node-version && run-s admin-generator intl:compile && run-p gql:types generate-block-types && dotenv -e .env.secrets -e .env.local -e .env -e .env.site-configs -- vite"
    },
    "dependencies": {
        "@apollo/client": "^3.13.8",
        "@comet/admin": "8.0.0",
        "@comet/admin-icons": "8.0.0",
        "@comet/cms-admin": "8.0.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fontsource-variable/roboto-flex": "^5.2.6",
        "@mui/material": "^7.1.2",
        "@mui/x-data-grid": "^7.29.7",
        "@mui/x-data-grid-pro": "^7.29.7",
        "@mui/x-date-pickers": "^7.29.4",
        "change-case": "^5.4.4",
        "date-fns": "^4.1.0",
        "final-form": "^4.20.10",
        "graphql": "^16.10.0",
        "history": "^4.10.1",
        "lodash.isequal": "^4.5.0",
        "rdndmb-html5-to-touch": "^8.1.2",
        "react": "^18.3.1",
        "react-app-polyfill": "^2.0.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dnd-multi-backend": "^8.1.2",
        "react-dom": "^18.3.1",
        "react-final-form": "^6.5.9",
        "react-intl": "^6.8.9",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4"
    },
    "devDependencies": {
        "@comet/admin-generator": "8.0.0",
        "@comet/cli": "8.0.0",
        "@comet/eslint-config": "8.0.0",
        "@cspell/eslint-plugin": "^8.19.4",
        "@formatjs/cli": "^6.7.2",
        "@graphql-codegen/add": "^5.0.3",
        "@graphql-codegen/cli": "^5.0.7",
        "@graphql-codegen/fragment-matcher": "^5.1.0",
        "@graphql-codegen/introspection": "^4.0.3",
        "@graphql-codegen/named-operations-object": "^3.1.1",
        "@graphql-codegen/near-operation-file-preset": "^3.1.0",
        "@graphql-codegen/typescript": "^4.1.6",
        "@graphql-codegen/typescript-operations": "^4.6.1",
        "@parcel/watcher": "^2.5.1",
        "@swc/plugin-emotion": "^10.0.2",
        "@types/lodash.isequal": "^4.5.8",
        "@types/node": "^22.14.1",
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react-swc": "^3.8.1",
        "check-node-version": "^4.2.1",
        "chokidar-cli": "^3.0.0",
        "dotenv-cli": "^7.4.4",
        "eslint": "^9.31.0",
        "knip": "^5.61.3",
        "npm-run-all2": "^7.0.2",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "typescript": "5.8.3",
        "vite": "^5.4.19",
        "vite-plugin-html": "^3.2.2"
    },
    "engines": {
        "node": "22"
    }
}
