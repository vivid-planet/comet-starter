{
    "name": "starter-admin",
    "private": true,
    "scripts": {
        "postinstall": "cd server && npm install",
        "start": "npm run intl:compile && run-p gql:types generate-block-types && dotenv -c secrets -- cross-env BABEL_ENV=development webpack serve --config ./webpack.config.ts --mode development",
        "build": "npm run intl:compile && run-p gql:types generate-block-types && cross-env BABEL_ENV=production webpack --config ./webpack.config.ts --mode production --progress",
        "serve": "cd server && npm run serve",
        "gql:types": "graphql-codegen",
        "gql:watch": "graphql-codegen --watch",
        "lint": "npm run intl:compile && run-p gql:types generate-block-types && run-p lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 --ext .ts,.tsx,.js,.jsx,.json,.md src/ package.json",
        "lint:tsc": "tsc --project .",
        "intl:extract": "formatjs extract \"src/**/*.ts*\" --ignore ./**.d.ts --out-file lang-extracted/en.json --format simple",
        "intl:compile:comet": "formatjs compile-folder --format simple --ast lang/comet-lang lang-compiled/comet-lang",
        "intl:compile:starter-admin": "formatjs compile-folder --format simple --ast lang/starter-lang/admin lang-compiled/starter-admin",
        "intl:compile": "run-p intl:compile:starter-admin intl:compile:comet",
        "generate-block-types": "comet generate-block-types --inputs",
        "generate-block-types:watch": "chokidar -s \"**/block-meta.json\" -c \"npm run generate-block-types\""
    },
    "dependencies": {
        "@apollo/client": "^3.2.5",
        "@babel/core": "^7.0.0",
        "@comet/admin": "^4.4.1",
        "@comet/admin-icons": "^4.4.1",
        "@comet/admin-rte": "^4.4.1",
        "@comet/admin-theme": "^4.4.1",
        "@comet/blocks-admin": "^4.4.1",
        "@comet/cms-admin": "^4.2.0",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@fontsource/roboto": "^4.5.8",
        "@mui/icons-material": "^5.8.4",
        "@mui/lab": "^5.0.0-alpha.88",
        "@mui/material": "^5.8.6",
        "@mui/styles": "^5.8.6",
        "@mui/system": "^5.8.6",
        "@mui/x-data-grid": "^5.17.26",
        "date-fns": "^2.28.0",
        "exceljs": "^3.4.0",
        "file-saver": "^2.0.2",
        "final-form": "^4.16.1",
        "graphql": "^15.4.0",
        "history": "^4.10.0",
        "lodash.isequal": "^4.0.0",
        "material-design-icons": "^3.0.1",
        "react": "^17.0.0",
        "react-app-polyfill": "^2.0.0",
        "react-dnd": "^16.0.0",
        "react-dnd-html5-backend": "^16.0.0",
        "react-dom": "^17.0.0",
        "react-final-form": "^6.3.1",
        "react-intl": "^5.10.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@comet/cli": "^4.4.1",
        "@comet/eslint-config": "^4.4.1",
        "@emotion/babel-plugin": "^11.0.0",
        "@formatjs/cli": "^3.0.1",
        "@gitbeaker/node": "^35.6.0",
        "@graphql-codegen/add": "^3.0.0",
        "@graphql-codegen/cli": "^2.0.0",
        "@graphql-codegen/fragment-matcher": "^2.0.0",
        "@graphql-codegen/introspection": "^2.0.0",
        "@graphql-codegen/named-operations-object": "^2.0.0",
        "@graphql-codegen/near-operation-file-preset": "^2.5.0",
        "@graphql-codegen/typescript": "^2.0.0",
        "@graphql-codegen/typescript-operations": "^2.0.0",
        "@types/lodash.isequal": "^4.0.0",
        "@types/node": "^18.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router": "^5.0.3",
        "@types/react-router-dom": "^5.1.7",
        "@types/webpack-env": "^1.13.9",
        "babel-loader": "^8.0.0",
        "babel-preset-react-app": "^10.0.0",
        "chokidar-cli": "^2.1.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.0.0",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.0.0",
        "html-webpack-plugin": "^5.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "style-loader": "^3.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^4.2.3",
        "webpack": "^5.0.0",
        "webpack-cli": "^4.0.0",
        "webpack-dev-server": "^4.0.0"
    }
}
