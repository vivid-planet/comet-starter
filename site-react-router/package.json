{
  "name": "my-react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "devx": "dotenv -e .env.secrets -e .env.local -e .env -e .env.site-configs -- react-router dev --port 3000",
    "dev": "dotenv -e .env.secrets -e .env.local -e .env -e .env.site-configs -- tsx server.ts",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "generate-block-types": "comet generate-block-types --output-file ./app/blocks.generated.ts",
    "generate-block-types:watch": "chokidar -s \"block-meta.json\" -c \"npm run generate-block-types\"",
    "gql:types": "graphql-codegen-esm --config codegen.cjs",
    "gql:watch": "graphql-codegen-esm --watch  --config codegen.cjs",
    "intl:compile": "formatjs compile-folder --format simple --ast lang/starter-lang/site lang-compiled/",
    "intl:extract": "formatjs extract \"src/**/*.ts*\" --ignore **/*.d.ts --out-file lang-extracted/en.json --format simple --throws"
  },
  "dependencies": {
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "@swc/plugin-styled-components": "^6.8.2",
    "@types/express": "^5.0.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cache-manager": "^5.0.0",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.3",
    "isbot": "^5.1.17",
    "lodash.isequal": "^4.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intl": "^7.1.6",
    "react-router": "^7.2.0",
    "redraft": "^0.10.2",
    "scroll-into-view-if-needed": "^3.1.0",
    "sitemap": "^8.0.0",
    "styled-components": "^6.1.15",
    "swiper": "^11.2.4",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@comet/cli": "7.13.0",
    "@formatjs/cli": "^6.6.1",
    "@graphql-codegen/add": "^3.0.0",
    "@graphql-codegen/cli": "^2.0.0",
    "@graphql-codegen/import-types-preset": "^2.0.0",
    "@graphql-codegen/named-operations-object": "^2.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.0",
    "@react-router/dev": "^7.2.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/draft-js": "^0.11.18",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "babel-plugin-styled-components": "^2.1.4",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-babel": "^1.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
